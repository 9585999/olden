{
  "name": "Olden",
  "version": "0.0.1",
  "main": "./app/main.js",
  "scripts": {
    "start": "electron app",
    "build": "node scripts/build.js && npm install --prefix ./app",
    "package_macos": "electron-packager . --platform=darwin --icon=assets/app_icon.icns --version=1.3.5 --app-version=0.0.1 --out=dist",
    "package_win": "electron-packager . --platform=win32 --arch=all --icon=assets/app_icon.ico --version=1.3.5 --app-version=0.0.1 --out=dist",
    "package_linux": "electron-packager . --platform=linux --arch=all --icon=assets/app_icon.png --version=1.3.5 --app-version=0.0.1 --out=dist",
    "bootstrap": "npm install -g electron-prebuilt electron-packager"
  },
  "dependencies": {
    "dexie": "^1.4.1"
  }
}
